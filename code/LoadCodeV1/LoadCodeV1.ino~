//Pin Assignments
//DAC Shift Register; 16 bit serial
#define DAC_c = 2, DAC_l = 3, DAC_d = 4;

//Display shift register; 6 bit serial (takes 8 bits)
#define DISP_c = 7, DISP_l = 6, DISP_d = 8;

